
@{
    ViewData["Title"] = "view_withdrawal_report";

	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
}

<div class="row merged20 mb-4">
	<div class="col-lg-8 mb-3">
		<div class="d-widget">
			<div class="d-widget-title">
				<h4>View Withdrawal Report	</h4>
			</div>

			<form class="row">


				<div class="form-group col-md-12">
					<div class="row align-items-center">
						<label class="col-md-4 form-label" for="from_date">From Date</label>
						<div class="col-md-8">
							<input type="date" class="form-control" id="from_date" name="from_date">
						</div>
					</div>
				</div>


				<div class="form-group col-md-12">
					<div class="row align-items-center">
						<label class="col-md-4 form-label" for="to_date">To Date</label>
						<div class="col-md-8">
							<input type="date" class="form-control" id="to_date" name="to_date">
						</div>
					</div>
				</div>

				<div class="form-group col-md-12">
					<div class="row align-items-center">
						<label class="col-md-4 form-label">Status</label>
						<div class="col-md-8">
							<select name="txtstatus" id="txtstatus" class="form-control ">
								<option value="">Select Status</option>
								<option value="1">Pending</option>
								<option value="2">Approved</option>
								<option value="3">Rejected</option>
								<option value="4">Processed</option>
							</select>
						</div>
					</div>
				</div>



				<div class="col-md-8	 offset-4">
					<button type="submit" class="button primary ">Submit</button>
					<button type="Reset" class="button danger">Reset</button>
				</div>









			</form>

		</div>
	</div>
	<div class="col-lg-12">
		<div class="d-widget mb-4">
			<table class="table table-default  table-responsive">
				<thead>
					<tr>
						<th>Sn</th>
						<th>User Id</th>
						<th>User Name</th>
						<th>Bank Name</th>
						<th>Account No</th>
						<th>IFSC Code</th>
						<th>Branch</th>
						<th>Amount</th>
						<th>Admin</th>
						<th>Status</th>
						<th>Action</th>
					</tr>
				</thead>
				<tbody>
					<tr>

						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>
						<td>Company</td>



				</tbody>


			</table>


		</div>

	</div>
</div>

