@{
	ViewData["Title"] = "AdminLogin";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="row merged20 mb-3">
	<div class="col-lg-12">
		<div class="d-widget">
			<div class="d-widget-title">
				<h4>Admin Credential</h4>
			</div>


			<form class="row">

				<div class="form-group col-md-12">
					<div class="row align-items-center">
						<label class="col-md-4 form-label">Login Id</label>
						<div class="col-md-8">
							<input type="text" class="form-control" name="txtintroducer_id" id="txtintroducer_id" onblur="get_intro_detail()" value="admin">
						</div>
					</div>
				</div>

				<div class="form-group col-md-12">
					<div class="row align-items-center">
						<label class="col-md-4 form-label">Password</label>
						<div class="col-md-8">
							<input class="form-control empty" id="txtpassword" name="txtpassword" value="123456" placeholder="Enter Password" type="password">
						</div>
					</div>
				</div>


				<div class="col-md-8 offset-4">



					<button type="submit" class="button primary">Update</button>



				</div>









			</form>



		</div>
	</div>
</div>