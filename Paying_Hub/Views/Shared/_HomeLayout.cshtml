<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title -PayRing Benefits</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Place favicon.ico in the root directory -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Home/favicon/favicon.png">

    <!-- CSS here -->
    <link rel="stylesheet" href="~/Home/assets/css/bootstrap.css">
    <link rel="stylesheet" href="~/Home/assets/css/animate.css">
    <link rel="stylesheet" href="~/Home/assets/css/swiper-bundle.css">
    <link rel="stylesheet" href="~/Home/assets/css/slick.css">
    <link rel="stylesheet" href="~/Home/assets/css/nouislider.css">
    <link rel="stylesheet" href="~/Home/assets/css/magnific-popup.css">
    <link rel="stylesheet" href="~/Home/assets/css/font-awesome-pro.css">
    <link rel="stylesheet" href="~/Home/assets/css/modernizr.css">
    <link rel="stylesheet" href="~/Home/assets/css/spacing.css">
    <link rel="stylesheet" href="~/Home/assets/css/main.css">
</head>
<body>

    <!-- pre loader area start -->
    <div id="loading">
        <div id="loading-center">
            <div id="loading-center-absolute">
                <!-- loading content here -->
                <div class="tp-preloader-content">
                    <div class="tp-preloader-logo">
                        <div class="tp-preloader-circle">
                            <svg width="190" height="190" viewBox="0 0 380 380" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <circle stroke="#D9D9D9" cx="190" cy="190" r="180" stroke-width="6" stroke-linecap="round"></circle>
                                <circle stroke="red" cx="190" cy="190" r="180" stroke-width="6" stroke-linecap="round"></circle>
                            </svg>
                        </div>
                        <img style="width:120px;" src="~/Home/favicon/favicon.png" alt="favicon">
                    </div>
                    <p class="tp-preloader-subtitle">Loading</p>
                </div>
            </div>
        </div>
    </div>
    <!-- pre loader area end -->
    <!-- back to top start -->
    <div class="back-to-top-wrapper">
        <button id="back_to_top" type="button" class="back-to-top-btn">
            <svg width="12" height="7" viewBox="0 0 12 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M11 6L6 1L1 6" stroke="currentColor" stroke-width="1.5" stroke-linecap="round"
                      stroke-linejoin="round" />
            </svg>
        </button>
    </div>
    <!-- back to top end -->
    <!-- header-area-start -->
    <header>
        <div class="tptransparent__header-4">
            <div class="header-top d-none d-sm-block">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-6 col-sm-8">
                            <!--div class="header-offer d-flex align-items-center">
                               <img src="assets/img/shape/header-rocket.png" alt="">
                               <p>End of Year Sale:<span>Save up to 35%</span> on Tasks</p>
                            </div-->
                        </div>
                        <div class="col-lg-6 col-sm-4">
                            <div class="header-social d-flex align-items-center">
                                <div class="header-social-item">
                                    <a href="#"><i class="fa-brands fa-facebook"></i></a>
                                    <a href="#"><i class="fa-brands fa-twitter"></i></a>
                                    <a href="#"><i class="fa-brands fa-instagram"></i></a>
                                    <a href="#"><i class="fa-brands fa-pinterest"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="main-header">
                <div class="custom-container">
                    <div id="header-sticky" class="header-bg-4">
                        <div class="row align-items-center">
                            <div class="col-xl-3 col-lg-6 col-sm-5 col-6">
                                <div class="tplogo__area">
                                    <a href="">
                                        <img src="~/Home/logo/logo.png" alt="logo">
                                    </a>
                                </div>
                            </div>
                            <div class="col-xl-7 col-lg-6  d-none d-xl-block">
                                <div class="tpmenu__area main-mega-menu">
                                    <nav class="tp-main-menu-content">
                                        <ul>
                                            <li class="active"><a asp-controller="Home" asp-action="Home">Home</a></li>
                                            <li><a asp-controller="Home" asp-action="about">About</a></li>
                                            <li><a asp-controller="Home" asp-action="Market">Market</a></li>
                                            <li><a asp-controller="Home" asp-action="Benefits">Benefits</a></li>
                                            <li><a asp-controller="Home" asp-action="FAQ">FAQ</a></li>
                                            <li><a asp-controller="Home" asp-action="Contact">Contact</a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                            <div class="col-xl-2 col-lg-6 col-sm-7 col-6">
                                <div class="header-btn header-btn-4 text-end">
                                    <a class="blue-btn d-none d-xl-block" asp-controller="Home" asp-action="Login">LogIn</a>
                                    <div class="offcanvas-btn d-xl-none ml-20">
                                        <button class="offcanvas-open-btn"><i class="fa-solid fa-bars"></i></button>
                                    </div>
                                </div>
                            </div>

                            <!--div class="col-xl-1 col-lg-6 col-sm-7 col-6">
                               <div class="header-btn header-btn-4 text-end">
                                  <a class="blue-btn d-none d-xl-block" href="registration.html">SignUp</a>
                               </div>
                            </div-->


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- header-area-end -->
    <div>
        @RenderBody()
    </div>
    <!-- footer-area-start -->
    <footer>
        <div class="footer-area footer-bg-4 pt-100">
            <div class="container">
                <div class="footer-top">
                    <div class="row">
                        <div class="col-lg-4 col-md-6 col-sm-12">
                            <div class="footer-widget footer-4-col-1 mb-30">
                                <div class="footer-widget-logo mb-20">
                                    <a href="index.html">
                                        <span>
                                            <img style="width:120px;" src="~/Home/logo/logo.png" alt="">
                                        </span>
                                    </a>
                                </div>
                                <div class="footer-widget-content">
                                    <p class="footer-widget-text mb-20">At The PayRing Emerging Capital, we believe in a direct and transparent approach to investment.</p>
                                    <div class="fooer-btn-4">
                                        <a class="blue-btn" href="#">Grow Traffic</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6 col-sm-6">
                            <div class="footer-widget  footer-4-col-2 pl-30 mb-40">
                                <h4 class="footer-widget-title mb-15">Information</h4>
                                <div class="footer-widget-link">
                                    <ul>
                                        <li class="active"><a asp-controller="Home" asp-action="Home">Home</a></li>
                                        <li><a asp-controller="Home" asp-action="about">About</a></li>
                                        <li><a asp-controller="Home" asp-action="Market">Market</a></li>
                                        <li><a asp-controller="Home" asp-action="Benefits">Benefits</a></li>
                                        <li><a asp-controller="Home" asp-action="FAQ">FAQ</a></li>
                                        <li><a asp-controller="Home" asp-action="Contact">Contact</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6 col-sm-8">
                            <div class="footer-widget  footer-4-col-4 mb-40">
                                <h4 class="footer-widget-title mb-20">Subcribe.</h4>
                                <div class="footer-widget-content mb-40">
                                    <p class="footer-widget-text mb-20"></p>
                                    <form action="#">
                                        <div class="p-relative">
                                            <div class="footer-widget-newsletter-input mb-15">
                                                <input type="email" placeholder="Your Email">
                                                <span>
                                                    <svg width="18" height="16" viewBox="0 0 18 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <path d="M13 14.6H5C2.6 14.6 1 13.4 1 10.6V5C1 2.2 2.6 1 5 1H13C15.4 1 17 2.2 17 5V10.6C17 13.4 15.4 14.6 13 14.6Z" stroke="#565764" stroke-width="1.5" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round" />
                                                        <path d="M13 5.39844L10.496 7.39844C9.672 8.05444 8.32 8.05444 7.496 7.39844L5 5.39844" stroke="#565764" stroke-width="1.5" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round" />
                                                    </svg>
                                                </span>
                                            </div>
                                            <div class="footer-widget-btn">
                                                <button class="footer-widget-newsletter-btn-2">
                                                    <i>
                                                        <svg width="10" height="10" viewBox="0 0 10 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                            <path d="M1 9L9 1" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                                                            <path d="M1 1H9V9" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                                                        </svg>
                                                    </i>
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class="footer-widget-social">
                                    <a href="#"><i class="fa-brands fa-facebook-f"></i></a>
                                    <a href="#"><i class="fa-brands fa-twitter"></i></a>
                                    <a href="#"><i class="fa-brands fa-linkedin-in"></i></a>
                                    <a href="#"><i class="fa-brands fa-instagram"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="footer-bottom-4">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="footer-widget-copyright footer-widget-copyright-4 text-center">
                                <span>© 2025 <a target="_blank" href="index.html">PayRing</a> All Rights Reserved.</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- footer-area-end -->
    <!-- JS here -->
    <script src="~/Home/assets/js/email-decode.min.js"></script>
    <script src="~/Home/assets/js/jquery.js"></script>
    <script src="~/Home/assets/js/waypoints.js"></script>
    <script src="~/Home/assets/js/bootstrap-bundle.js"></script>
    <script src="~/Home/assets/js/meanmenu.js"></script>
    <script src="~/Home/assets/js/swiper-bundle.js"></script>
    <script src="~/Home/assets/js/slick.js"></script>
    <script src="~/Home/assets/js/nouislider.js"></script>
    <script src="~/Home/assets/js/magnific-popup.js"></script>
    <script src="~/Home/assets/js/parallax.js"></script>
    <script src="~/Home/assets/js/nice-select.js"></script>
    <script src="~/Home/assets/js/wow.js"></script>
    <script src="~/Home/assets/js/isotope-pkgd.js"></script>
    <script src="~/Home/assets/js/imagesloaded-pkgd.js"></script>
    <script src="~/Home/assets/js/tilt.jquery.min.js"></script>
    <script src="~/Home/assets/js/purecounter.js"></script>
    <script src="~/Home/assets/js/ajax-form.js"></script>
    <script src="~/Home/assets/js/parallax-scroll.js"></script>
    <script src="~/Home/assets/js/TweenMax.min.js"></script>
    <script src="~/Home/assets/js/wavify.js"></script>
    <script src="~/Home/assets/js/animate-headline.js"></script>
    <script src="~/Home/assets/js/main.js"></script>
</body>
</html>
